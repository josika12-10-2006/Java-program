import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();  

        if (n < 9) {

            for (int i = 1; i <= n; i++) {

                System.out.println("5 x " + i + " = " + (5 * i));

            }

        } else {

            System.out.println("Invalid Input");

        }

    }

}
