import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        int tickets = sc.nextInt();

        sc.nextLine(); 

        String refreshment = sc.nextLine().trim().toLowerCase();

        String coupon = sc.nextLine().trim().toLowerCase();

        String type = sc.nextLine().trim().toLowerCase();        if (tickets < 5 || tickets > 40) {

            System.out.println("Minimum of 5 and Maximum of 40 Tickets");

            return;
}
        double ticketCost;

        if (type.equals("k")) {

            ticketCost = 75;

            
        } else if (type.equals("q")) {

            ticketCost = 150;

        } else {

            System.out.println("Invalid Input");

            return;
        }
          double total = tickets * ticketCost;
       if (tickets > 20) {
      total -= total * 0.10;

        }

        if (coupon.equals("y")) {

            total -= total * 0.02;

        }
       if (refreshment.equals("y")) {
       total += 50 * tickets;
 }
 System.out.printf("Ticket cost:%.2f\n", total);

    }

}
